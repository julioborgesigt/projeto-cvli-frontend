{
  "name": "projeto-cvli-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "dev": "concurrently \"npm:dev:vite\" \"npm:dev:electron\"",
    "build": "vite build",
    "pack": "npm run build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cors": "^2.8.5",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "maska": "^3.2.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "vite": "^7.1.7"
  },
  "build": {
  "appId": "com.seu-nome.projetocvli",
  "productName": "Projeto CVLI",
  "files": [
    "dist/**/*",
    "electron-main.cjs"
  ],
  "directories": {
    "output": "release"
  },
  "win": {
    "target": "nsis"
  },
  "mac": {
    "target": "dmg"
  },
  "linux": {
    "target": "AppImage"
  }
}
}
